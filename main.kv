#:import utils kivy.utils
#:include TTT.kv

BoxLayout:
    orientation:'vertical'
    MDToolbar:
        id: toolbar
        pos_hint: {"top": 1}
        title: "Tic-Tac-Toe"
        md_bg_color: utils.get_color_from_hex("#e28c06")
        elevation: 10
        right_action_items: [['account-circle-outline', lambda x: print("hello")]]

    MDBottomNavigation:
        panel_color: .2, .2, .2, 1
        MDBottomNavigationItem:
            name: 'games'
            id: "games_bottom"
            icon: 'gamepad-variant-outline'
            TTT:
                name: 'ttt_name'
                id: 'ttt_id'







